{
	"initial:before": {
		"loopback#favicon": {}
	},
	"initial": {
		"compression": {},
		"cors": {
			"params": {
				"origin": true,
				"credentials": true,
				"maxAge": 86400
			}
		}
	},
	"session": {},
	"auth": {},
	"parse": {
		"body-parser#json": {
			"params": {
				"limit": "50mb"
			}
		},
		"body-parser#urlencoded": {
			"params": {
				"limit": "50mb",
				"extended": true
			}
		}
	},
	"routes": {
		"loopback#rest": {
			"paths": [
				"${restApiRoot}"
			]
		}
	},
	"files": {
		"loopback#static": {
			"params": "$!../client"
		}
	},
	"final": {
		"loopback#urlNotFound": {}
	},
	"final:after": {
		"loopback#errorHandler": {}
	}
}